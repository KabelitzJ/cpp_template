{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++: Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/bin/cpp_template.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb.exe",
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
      ]
    }
  ]
}
